<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Theme.FitnessKitTest" parent="Theme.MaterialComponents.DayNight.NoActionBar">
        <!-- Primary brand color. -->
        <item name="colorPrimary">@color/purple_500</item>
        <item name="colorPrimaryVariant">@color/purple_700</item>
        <item name="colorOnPrimary">?color_white</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">@color/teal_200</item>
        <item name="colorSecondaryVariant">@color/teal_700</item>
        <item name="colorOnSecondary">@color/black</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor">@color/black</item>
        <!-- Customize your theme here. -->
        <item name="color_black">@color/black</item>
        <item name="color_grey1">@color/grey1</item>
        <item name="color_grey2">@color/grey2</item>
        <item name="color_grey3">@color/grey3</item>
        <item name="color_orange">@color/orange</item>
        <item name="color_green">@color/green</item>
        <item name="color_white">@color/white</item>

    </style>
</resources>